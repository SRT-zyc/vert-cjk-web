{
    "name": "Vertical Line-ize CJK",
    "description": "Make web lines vertical for CJK (Chinese, Japanese, Korean) traditional",
    "manifest_version": 2,
    "version": "0.3.0",
    "homepage_url": "https://garywill.github.io",
    "developer": {
        "name": "Garywill",
        "url": "https://garywill.github.io"
    },
    "permissions": [
        "storage",
        "activeTab",
        "tabs",
        "*://*/*"
    ],
 
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_title": "Vertical Line-ize CJK"
    },
    "web_accessible_resources": [
        "wqy-microhei-rotate90cjk.ttf"
    ],
    "commands": {
        "_execute_browser_action": {
        }
    }
} 
